{
	"name": "framework/validation",
	"type": "library",
	"description": "The Validation package",
	"keywords": [
		"validation"
	],
	"homepage": "https://gitlab.com/the-framework/validation",
	"license": "proprietary",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"framework/language": "dev-master",
		"natanfelles/array-simple": "^0.1"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"apigen/apigen": "dev-master#85290e8",
		"framework/coding-standard": "dev-master",
		"localheinz/composer-normalize": "^1.1",
		"phpmd/phpmd": "^2.6",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^7.5"
	},
	"autoload": {
		"psr-4": {
			"Framework\\Validation\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Validation\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://the-framework.gitlab.io/satis/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-install-cmd": [
			".hooks/init"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/validation/issues",
		"wiki": "https://gitlab.com/the-framework/packages/validation",
		"source": "https://gitlab.com/the-framework/validation",
		"docs": "https://the-framework.gitlab.io/the-framework/packages/validation/docs"
	}
}
